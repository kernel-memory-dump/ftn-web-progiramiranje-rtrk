package primer04;

public class Osoba {
	private String ime;
	private String tip;
	public Osoba(String ime, String tip) {
		super();
		this.ime = ime;
		this.tip = tip;
	}
	public String getIme() {
		return ime;
	}
	public void setIme(String ime) {
		this.ime = ime;
	}
	public String getTip() {
		return tip;
	}
	public void setTip(String tip) {
		this.tip = tip;
	}
	
	
}
